--- Programa principal
function _init()
    c = SnakeCharacter({35,32,34,33},{52,52,51,51},30,30,1)
    tv = TerrainVector(8,16,16) -- green
    pf = PickUpFactory(21,c,tv)
    pf:create(5)
end 

function _update()
    c:move_player()
    pf:checkStatus(1)
end 

function _draw()
    cls()
    map(0,0,0,0,16,16)
    pf:draw()
    c:draw()
    print(c.overlaps,10,2,14)
    print(c.overlaps,11,2,8)
    
   
end 



